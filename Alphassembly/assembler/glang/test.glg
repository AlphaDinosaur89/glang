include "io.glg"
include "types.glg"

class Animal:
    test
    def constructor(this, val):
        var this.test = val
    end

    def sound(this):
        println("The animal makes a sound")
    end
end

class Dog(class Animal):
    def bark(this):
        println(this.test)
        #this.sound()
    end
end

var a = new(class Dog, "balls")

a.bark()
